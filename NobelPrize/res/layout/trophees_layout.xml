<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/TextViewTrophees_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/MesTrophées_title"
         />
    <TextView
	    android:id="@+id/TextViewTrophees_1"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:gravity="left"
	    android:text="@string/MesTrophées_1"
	    android:textSize="24sp"
	    android:background="@drawable/back"
	    android:layout_margin="2dp"
	    android:drawableRight="@android:drawable/btn_star_big_off"/>
    <TextView
	    android:id="@+id/TextViewTrophees_2"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:gravity="left"
	    android:text="@string/MesTrophées_2"
	    android:textSize="24sp"
	    android:background="@drawable/back"
	    android:layout_margin="2dp"
	    android:drawableRight="@android:drawable/btn_star_big_off"/>
    <TextView
	    android:id="@+id/TextViewTrophees_3"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:gravity="left"
	    android:text="@string/MesTrophées_3"
	    android:textSize="24sp"
	    android:background="@drawable/back"
	    android:layout_margin="2dp"
	    android:drawableRight="@android:drawable/btn_star_big_off"/>
    <TextView
	    android:id="@+id/TextViewTrophees_4"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:gravity="left"
	    android:text="@string/MesTrophées_4"
	    android:textSize="24sp"
	    android:background="@drawable/back"
	    android:layout_margin="2dp"
	    android:drawableRight="@android:drawable/btn_star_big_off"/>
    <TextView
	    android:id="@+id/TextViewTrophees_5"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:gravity="left"
	    android:text="@string/MesTrophées_5"
	    android:textSize="24sp"
	    android:background="@drawable/back"
	    android:layout_margin="2dp"
	    android:drawableRight="@android:drawable/btn_star_big_off"/>

</LinearLayout>